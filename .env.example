# ---- Docker Compose env (copy to .env to override defaults) ----

BACKEND_PORT=8000
FRONTEND_PORT=5173

# CORS origins for backend
CORS_ORIGINS=http://localhost:5173

# Frontend -> Backend API base URL used by Vite dev server (browser)
VITE_API_BASE=http://localhost:8000

# Timezone
TZ=Asia/Taipei

# Backend auth (used by backend container)
SECRET_KEY=change-this-in-production
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123
